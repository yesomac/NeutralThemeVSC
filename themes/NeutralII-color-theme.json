        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#bbd6b2"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
